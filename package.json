    "@types/express": "^5.0.0",
    "@types/node": "^22.8.0",
    "concurrently": "^9.0.1",
    "nodemon": "^3.1.7",
    "prisma": "^5.21.1",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
